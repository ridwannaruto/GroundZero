{% extends "::base.html.twig" %}

{% block title %}Volma - Track Record{% endblock %}

{% block body %}



<section class="container">
<div class="notify">
<h2 class="price">Volunteer Track Record</h2>  <br><br>

    <table width="90%">
        <tr>
    <th width="18%" height="33" scope="row">&nbsp;</th>
    <td>&nbsp;</td>
    <td>&nbsp;</td>
    <td>&nbsp;</td>
    <td>&nbsp;</td>
  </tr>
        <tr>
    <td height="34">&nbsp;</td>
    <td><strong>Name</strong></td>
    <td>{{fname}} {{lname}}</td>
    
  </tr>
         <tr>
    <td height="34">&nbsp;</td>
    <td><strong>Joined Date</strong></td>
    <td>{{jdate|date("m/d/Y")}}</td>
    
  </tr>
         <tr>
    <td height="34">&nbsp;</td>
    <td><strong>Overall Rating</strong></td>
    <td>{{overallrating}}</td>
    
  </tr>
    </table>
</div>
        </section>
        
    
    
<p align ="justify"></p>
<p> <div class="inner">
    <h2 class="price">Comments</h2>
 <table width="398" border="10" align="center" cellpadding="0">
   {% for n in comments %}
        <p>
        
        <div class="notify" >
        <p align="center"><strong>"{{n}}"</strong></p><p>
              
        
        </div>
        </p>
    {% endfor %}
        
       
</table></div></p>

<p align ="justify"></p>
<p> <div class="inner">
    <h2 class="price">Projects Participated</h2>
 <table width="398" border="10" align="center" cellpadding="0">
   {% for n in projects %}
        <p>
        
        <div class="notify" >
        <p align="center"><strong></strong></p><p>
       <strong>Project Name: </strong>{{ n.getName() }}<br>
       <strong>Details: </strong>{{ n.getDescription() }}<br>
        
        </div>
        </p>
    {% endfor %}
        
       
</table></div></p>


<p> </p>
    
{% endblock %}
